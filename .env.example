# W&B Weave Agent Project Environment Variables

# Required: W&B API Key
WANDB_API_KEY=your_wandb_api_key_here

# Required: OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# Optional: W&B Project Settings
WANDB_PROJECT=weave-agent-project
WANDB_ENTITY=your_wandb_entity

# Optional: Model Configuration
DEFAULT_MODEL=gpt-4
MAX_TOKENS=1000

# Optional: Agent Configuration
MEMORY_FILE=agent_memory.json
MAX_MEMORIES=1000

# Optional: OpenPipe RL Training
OPENPIPE_API_KEY=your_openpipe_api_key_here

# Optional: Serper API for real web search
SERPER_API_KEY=your_serper_api_key_here
